    "jest": {
        "rootDir": ".",
        "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.tsx?$",
        "moduleDirectories": [
            "node_modules",
            "bower_components",
            "src"
        ],
        "setupFilesAfterEnv": [
            "<rootDir>/src/utils/jest/setupTest.js"
        ],
        "moduleNameMapper": {
            "\\.(css|jpg|png|svg|less)$": "<rootDir>/src/utils/jest/styleMock.js",
            "nav-(.*)-style": "<rootDir>/src/utils/jest/styleMock.js"
        }
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
