    "scripts": {
        "start": "npm-run-all -p -r build-client-and-watch build-server-and-watch start-server-and-watch typecheck-watch",
        "start-server-and-watch": "nodemon build/server.js",
        "build": "npm-run-all -p -r build-client && npm run build-server",
        "build-client": "cross-env NODE_ENV=production webpack -p --display-error-details --config webpack.client.config.js",
        "build-server": "cross-env NODE_ENV=production webpack -p --display-error-details --config webpack.server.config.js",
        "build-client-and-watch": "cross-env NODE_ENV=development webpack --display-error-details --config webpack.client.config.js --watch",
        "build-server-and-watch": "cross-env NODE_ENV=development webpack --display-error-details --config webpack.server.config.js --watch",
        "test": "jest",
        "typecheck": "tsc",
        "typecheck-watch": "tsc -watch",
        "test-dev": "jest --watch",
        "lint": "eslint ./src/**/*.{js,ts,tsx}",
        "lint:fix": "eslint ./src/**/*.{js,ts,tsx} --fix",
        "docker-build": "docker build . -t navikt/nav-dekoratoren",
        "docker-push": "docker push navikt/nav-dekoratoren",
        "docker-start": "docker run --rm -p 8088:8088 navikt/nav-dekoratoren"
    },
